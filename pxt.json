{
  "name": "BitBot",
  "version": "1.3.0",
  "description": "Microsoft MakeCode package for 4tronix BitBot robot",
  "license": "MIT",
  "dependencies": {
    "core": "*",
    "neopixel": "github:microsoft/pxt-neopixel#v0.6.10"
  },
  "files": [
    "README.md",
    "main.ts",
    "_locales/no/BitBot-strings.json",
    "_locales/no/BitBot-jsdoc-strings.json",
    "_locales/fr/BitBot-strings.json",
    "_locales/fr/BitBot-jsdoc-strings.json",
    "_locales/bluetooth-jsdoc-strings.json",
    "_locales/bluetooth-strings.json",
    "enums.d.ts",
    "shims.d.ts",
    "bluetooth.ts",
    "bluetooth.cpp",
    "BLEHF2Service.h",
    "BLEHF2Service.cpp"
  ],
  "testFiles": [
    "tests.ts"
  ],
  "yotta": {
    "config": {
      "microbit-dal": {
        "bluetooth": {
          "enabled": 1
        }
      }
    },
    "optionalConfig": {
      "microbit-dal": {
        "stack_size": 1280,
        "gatt_table_size": "0x700"
      }
    }
  },

  "public": true
}
